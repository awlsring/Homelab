{
  "name": "cdktf",
  "version": "1.0.0",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get -o src/.gen",
    "build": "tsc",
    "synth": "npm run compile && cdktf synth",
    "deploy": "npm run compile && cdktf synth && cdktf deploy",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@awlsring/cdktf-github-actions": "^0.0.3",
    "@awlsring/cdktf-proxmox": "^0.0.12",
    "@awlsring/cdktf-surreal-backend": "^0.0.6",
    "cdktf": "^0.14.3",
    "constructs": "^10.1.52",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
